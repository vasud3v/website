# API Keys for Upload Services
# Copy this file to .env and fill in your actual values

# Streamtape API Credentials
# Get from: https://streamtape.com/account
STREAMTAPE_LOGIN=your_streamtape_login_here
STREAMTAPE_API_KEY=your_streamtape_api_key_here

# Lulustream API Credentials
# Get from: https://lulustream.com/account
LULUSTREAM_API_KEY=your_lulustream_api_key_here

# StreamWish API Credentials
# Get from: https://streamwish.com/account
STREAMWISH_API_KEY=your_streamwish_api_key_here

# Optional: Configuration
MAX_VIDEOS=999999
MAX_WORKERS=32
